<% unless @results.blank? %>
  $("#projects-search-results").html("");
  $("#projects-search-results").append("<%= escape_javascript(render 'projects/search_results') %>")
<% else %>
  $("#error-message-show").html("");
  $("#error-message-show").show();
  $("#error-message-show").html("Sorry no Results match with your search");
  setTimeout(function(){
    $("#error-message-show").fadeOut();
  }, 5000);
<%end %>
