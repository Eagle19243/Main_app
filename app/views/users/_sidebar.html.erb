<div class="small-12 medium-2 columns user-sidebar" data-equalizer-watch>
  <% if current_user %>
    <ul id="sidelist" class="tabs vertical tabs-index hidden-for-small-only sideul">
      <li class="tab-title">
        <%= link_to "Profile", user_path(current_user), data: { no_turbolink: true } %>
      </li>
      <li class="tab-title">
        <%= link_to "Projects", my_projects_path, data: { no_turbolink: true } %>
      </li>
      <li class="tab-title">
        <%= link_to "Notifications", notifications_path, data: { no_turbolink: true } %>
      </li>
      <li class="tab-title">
        <%= link_to "Messages", messages_path, data: { no_turbolink: true } %>
      </li>
    </ul>
  <% end %>
</div>